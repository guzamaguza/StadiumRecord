<% index = 0 %>
<% @arenas.each do |value| %>
  <h3> <%=index + 1%>) <a href="https://en.wikipedia.org/wiki/<%=value[:name].gsub(/\s+/, '_')%>"><%=value[1][:name]%></a>, <%=value[1][:location]%>, <%=value[1][:team]%> </h3>
<% end %>
